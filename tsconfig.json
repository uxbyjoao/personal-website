{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@data": [
        "src/data/data"
      ],
      "@utils": [
        "src/utils"
      ],
      "@types": [
        "src/types"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}